{#def metadata, ui, record, extra_context, d #}
{% extends "oarepo_ui/detail.html" %}
{%- set title = "Debug | Vocabulary Detail" %}

{% block record_main_content %}
<div class="debug-output">
  <h2>metadata</h2>
  <pre>{{ metadata | tojson(indent=2) }}</pre>

  <h2>ui</h2>
  <pre>{{ ui | tojson(indent=2) }}</pre>

  <h2>record</h2>
  <pre>{{ record | tojson(indent=2) }}</pre>

  <h2>extra_context</h2>
  <pre>{{ extra_context | tojson(indent=2) }}</pre>

  <h2>d</h2>
  <pre>{{ d | tojson(indent=2) }}</pre>
</div>
{% endblock %}