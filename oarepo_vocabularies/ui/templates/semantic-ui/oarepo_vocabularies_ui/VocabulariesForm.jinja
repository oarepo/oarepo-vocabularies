{#def record, api_record, form_config, extra_context, data, d #}
{%- set title = "Debug | Vocabulary Form" %}

{% block record_main_content %}
<div class="debug-output">
  <h2>record</h2>
  <pre>{{ record | tojson(indent=2) }}</pre>

  <h2>api_record</h2>
  <pre>{{ api_record | tojson(indent=2) }}</pre>

  <h2>form_config</h2>
  <pre>{{ form_config | tojson(indent=2) }}</pre>

  <h2>extra_context</h2>
  <pre>{{ extra_context | tojson(indent=2) }}</pre>

  <h2>data</h2>
  <pre>{{ data | tojson(indent=2) }}</pre>

  <h2>d</h2>
  <pre>{{ d | tojson(indent=2) }}</pre>
</div>
{% endblock %}